{
	"name": "How long?",
	"version": "0.4",
	"description": "Shows when you can leave!",
	"permissions": [
		"activeTab"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"browser_action": {
		"default_title": "How long?",
		"default_icon": {
			"16": "howlong16.png",
			"32": "howlong32.png",
			"128": "howlong128.png"
		}
	},
	"manifest_version": 2
}
